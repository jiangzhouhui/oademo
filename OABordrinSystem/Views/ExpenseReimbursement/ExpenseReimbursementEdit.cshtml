                            <tbody id="ReimbursementItemTr">
                                <tr>
                                    <td>
                                        <input type="hidden" class="Id" />
                                        <input type="text" class="form-control b_Date" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)">
                                    </td>
                                    <td>
                                        <select class="form-control b_CategoryNumber"></select>
                                    </td>
                                    <td>
                                        <select class="form-control b_ProjectName"></select>
                                        @*<input type="text" class="form-control b_ProjectName">*@
                                    </td>
                                    <td>
                                        <input type="text" class="form-control b_BudgetNumber" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)">
                                    </td>
                                    <td>
                                        <select class="form-control b_Currency">
                                            <option>无</option>
                                            <option value="人民币">人民币</option>
                                            <option value="美元">美元</option>
                                        </select>
                                    </td>
                                    <td>
                                        <input type="text" class="form-control b_Rate" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)">
                                    </td>
                                    <td>
                                        <input type="text" class="form-control b_OriginalCurrency" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)">
                                    </td>
                                    <td>
                                        <input type="text" class="form-control b_Count" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)">
                                    </td>
                                    <td>
                                        <select type="text"  class="form-control b_TaxRate" ></select>
                                        @*<input type="text" class="form-control b_TaxRate" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)">*@
                                    </td>
                                    <td>
                                        <input type="text" class="form-control b_Tax" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)">
                                    </td>
                                    <td>
                                        <input type="text" class="form-control b_TaxFreeAmount" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)">
                                    </td>
                                    <td>
                                        <label class="control-label b_CNYSubtotal"></label>
                                    </td>
                                    <td style="text-align:center;line-height:30px;">
                                        <a class="glyphicon glyphicon-plus AddReimbursementItem"></a>
                                        &nbsp;&nbsp;
                                        <a class="glyphicon glyphicon-minus deleteReimbursementItem"></a>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("合计(大写)", "b_ExpenseReimbursement", "ERItemType", ViewBag.language)</label>
                        <div class="col-sm-3">
                            <input type="text" class="form-control" name="b_AmountInWords" disabled="disabled" />
                        </div>

                        <label class="col-sm-2 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("合计(小写)", "b_ExpenseReimbursement", "ERItemType", ViewBag.language)</label>
                        <div class="col-sm-3">
                            <input type="text" class="form-control" name="b_TotalAmount" disabled="disabled" />

                        </div>
                    </div>
                    <div id="editLoanItemDetails" style="display:none;">
                        <h5 style="font-weight: bold;border-bottom: 1px solid #bcbcbc;padding-bottom: 4px;text-align: left;">
                            @OABordrinCommon.Common.GetLanguageValueByParam("借款明细", "b_ExpenseReimbursement", "ERItemType", ViewBag.language)
                        </h5>
