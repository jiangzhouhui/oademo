                            <label style="padding-top:5px;"><input class="i-checks" type="radio" name="b_IsBudgetary" value="true"> <i></i>&nbsp;@OABordrinCommon.Common.GetLanguageValueByParam("是", "ERCommon", "ERItemType", ViewBag.language)&nbsp;</label>
                            <label><input class="i-checks" type="radio" name="b_IsBudgetary" value="false"> <i></i>&nbsp;@OABordrinCommon.Common.GetLanguageValueByParam("否", "ERCommon", "ERItemType", ViewBag.language)&nbsp;</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-1 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("报销类型", "b_ExpenseReimbursement", "ERItemType", ViewBag.language)</label>
                        <div class="col-sm-3">
                            <label style="padding-top:5px;"><input class="i-checks" type="radio" name="b_Type" value="Non Project"> <i></i>&nbsp;@OABordrinCommon.Common.GetLanguageValueByParam("非项目", "b_ExpenseReimbursement", "ERItemType", ViewBag.language)&nbsp;</label>
                            <label><input class="i-checks" type="radio" name="b_Type" value="Project"> <i></i>&nbsp;@OABordrinCommon.Common.GetLanguageValueByParam("项目", "b_ExpenseReimbursement", "ERItemType", ViewBag.language)&nbsp;</label>
                        </div>
                        <label class="col-sm-1 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("代申请人", "b_ExpenseReimbursement", "ERItemType", ViewBag.language)</label>
