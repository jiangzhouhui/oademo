<div tabindex="-1" class="modal fade in" id="AgentSetEdit" role="dialog" aria-hidden="true" style="padding-right: 17px;">
    <div class="modal-dialog modal-lg" style="max-width:600px;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="exampleModalLabel1">@OABordrinCommon.Common.GetLanguageValueByParam("新增", "CommonName", "Common", ViewBag.language)</h4>
            </div>
            <div class="errorMessage fa fa-exclamation-triangle" style="background-color: #f4eedc; width:100%;padding:5px; display: none;"><span style="margin-left:10px;"></span></div>
            <div class="modal-body">
                <form class="form-horizontal">
                    <input type="hidden" name="id" />
                    <div class="form-group">
                        <label class="col-sm-2 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("委托人", "b_AgentSet", "Common", ViewBag.language)</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="b_DelegateName" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" isnotnull="true" disabled="disabled">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("代理人", "b_AgentSet", "Common", ViewBag.language)</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="b_AgentName" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" isnotnull="true">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("生效时间", "b_AgentSet", "Common", ViewBag.language)</label>
                        <div class="col-sm-10">
                            <div class="col-sm-6" style="padding-left:0px;">
                                <input type="text" class="form-control" name="b_StartDate" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" isnotnull="true">
                            </div>
                            <div class="col-sm-6" style="padding-left:0px;">
                                <div class="input-group clockpicker" data-autoclose="true">
                                    <input type="text" class="form-control" name="b_StartDateMinute">
                                    <span class="input-group-addon">
                                        <span class="fa fa-clock-o"></span>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("终止时间", "b_AgentSet", "Common", ViewBag.language)</label>
                        <div class="col-sm-10">
                            <div class="col-sm-6" style="padding-left:0px;">
                                <input type="text" class="form-control" name="b_EndDate" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" isnotnull="true">
                            </div>
                            <div class="col-sm-6" style="padding-left:0px;">
                                <div class="input-group clockpicker" data-autoclose="true">
                                    <input type="text" class="form-control" name="b_EndDateMinute">
                                    <span class="input-group-addon">
                                        <span class="fa fa-clock-o"></span>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("代理模块", "b_AgentSet", "Common", ViewBag.language)</label>
                        <div class="col-sm-10" id="AgentModuleList">

                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("是否有效", "b_AgentSet", "Common", ViewBag.language)</label>
                        <div class="col-sm-10">
                            <select class="form-control" name="b_IsValid">
                                <option value="1">是</option>
                                <option value="0">否</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("代理原因", "b_AgentSet", "Common", ViewBag.language)</label>
                        <div class="col-sm-10">
                            <textarea class="form-control" name="b_AgentReason" rows="6"></textarea>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button class="btn btn-white" type="button" data-dismiss="modal">@OABordrinCommon.Common.GetLanguageValueByParam("关 闭", "CommonName", "Common", ViewBag.language)</button>
                <button class="btn btn-primary" type="button" id="SaveAgentSet" operation="save">@OABordrinCommon.Common.GetLanguageValueByParam("保 存", "CommonName", "Common", ViewBag.language)</button>
            </div>
        </div>
    </div>
</div>




