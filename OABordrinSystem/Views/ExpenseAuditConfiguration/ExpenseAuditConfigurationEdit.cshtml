<div class="modal" tabindex="-1" id="ExpenseAuditConfigurationEdit" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" style="max-width:800px;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="exampleModalLabel1">@OABordrinCommon.Common.GetLanguageValueByParam("新增", "CommonName", "Common", ViewBag.language)</h4>
            </div>
            <div class="errorMessage fa fa-exclamation-triangle" style="background-color: #f4eedc; width:100%;padding:5px; display: none;"><span style="margin-left:10px;"></span></div>
            <div class="modal-body">
                <form class="form-horizontal">
                    <input type="hidden" name="Id" />
                    <div class="form-group">
                        <label class="col-sm-2 control-label">角色名称</label>
                        <div class="col-sm-4">
                            <select class="form-control" name="b_RoleName" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" isnotnull="true">
                                <option value="">请选择</option>
                                <option value="财务分析员">财务分析员</option>
                                <option value="费用会计">费用会计</option>
                            </select>
                        </div>
                        <label class="col-sm-2 control-label">类型</label>
                        <div class="col-sm-4">
                            <select name="b_Type" class="form-control" >
                                <option value="Non Project">非项目</option>
                                <option value="Project">项目</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">公司代码</label>
                        <div class="col-sm-4">
                            <select class="form-control" name="b_CompanyCode" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" isnotnull="true">
                            </select>
                        </div>
                        <div id="nProjectName">
                            <label class="col-sm-2 control-label">项目名称</label>
                            <div class="col-sm-4">
                                <select class="form-control" type="text" name="b_ProjectName" ></select>
                            </div>
                        </div>
                 
                    </div>
                    <div class="form-group" id="b_CostCenters" >
                        <label class="col-sm-2 control-label">成本中心</label>
                        <div class="col-sm-10">
                            <select data-placeholder="选择成本中心" name="b_CostCenters" class="chosen-select" multiple style="width:350px;display:none;" tabindex="4">
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">处理人</label>
                        <div class="col-sm-10">
                            <textarea class="form-control" name="b_HandlePersons" id="b_HandlePersons" rows="6"></textarea>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button class="btn btn-white" type="button" data-dismiss="modal">@OABordrinCommon.Common.GetLanguageValueByParam("关 闭", "CommonName", "Common", ViewBag.language)</button>
                <button class="btn btn-primary" type="button" id="SaveExpenseAuditConfiguration" operation="save">@OABordrinCommon.Common.GetLanguageValueByParam("保 存", "CommonName", "Common", ViewBag.language)</button>
            </div>
        </div>
    </div>
</div>
