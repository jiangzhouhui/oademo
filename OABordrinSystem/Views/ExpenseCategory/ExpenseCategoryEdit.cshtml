<div tabindex="-1" class="modal fade in" id="ExpenseCategoryEdit" role="dialog" aria-hidden="true" style="padding-right: 17px;">
    <div class="modal-dialog modal-lg" style="max-width:600px;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="exampleModalLabel1">@OABordrinCommon.Common.GetLanguageValueByParam("新增", "CommonName", "Common", ViewBag.language)</h4>
            </div>
            <div class="errorMessage fa fa-exclamation-triangle" style="background-color: #f4eedc; width:100%;padding:5px; display: none;"><span style="margin-left:10px;"></span></div>
            <div class="modal-body">
                <form class="form-horizontal">
                    <input type="hidden" name="Id" />
                    <div class="form-group">
                        <label class="col-sm-3 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("费用名称", "b_ExpenseCategory", "ERItemType", ViewBag.language)</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control" name="b_CostName" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" isnotnull="true">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("科目名称", "b_ExpenseCategory", "ERItemType", ViewBag.language)</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control" name="b_CorresponSubject" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" isnotnull="true">
                        </div>
                    </div>
        
                </form>
            </div>
            <div class="modal-footer">
                <button class="btn btn-white" type="button" data-dismiss="modal">@OABordrinCommon.Common.GetLanguageValueByParam("关 闭", "CommonName", "Common", ViewBag.language)</button>
                <button class="btn btn-primary" type="button" id="SaveExpenseCategory" operation="save">@OABordrinCommon.Common.GetLanguageValueByParam("保 存", "CommonName", "Common", ViewBag.language)</button>
            </div>
        </div>
    </div>
</div>
