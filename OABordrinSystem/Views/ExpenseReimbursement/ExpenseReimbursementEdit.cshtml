                        <div class="col-sm-3" style="display:none;">
                            <input type="text" class="form-control" name="b_Position" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" />
                        </div>
                        <label class="col-sm-1 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("申请部门", "b_ExpenseReimbursement", "ERItemType", ViewBag.language)</label>
                        <div class="col-sm-3">
                            <input type="text" class="form-control" name="b_Dept" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" isnotnull="true" disabled="disabled" />
                        </div>
                        <label class="col-sm-1 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("成本中心代码", "b_ExpenseReimbursement", "ERItemType", ViewBag.language)</label>
                        <div class="col-sm-3">
                            <input type="text" class="form-control" name="b_CostCenter" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" isnotnull="true" disabled="disabled" />
                        </div>
                        <label class="col-sm-1 control-label">@OABordrinCommon.Common.GetLanguageValueByParam("电话", "b_ExpenseReimbursement", "ERItemType", ViewBag.language)</label>
