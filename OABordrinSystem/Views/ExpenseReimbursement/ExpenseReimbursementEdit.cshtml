                        <div class="col-sm-3">
                            <input type="text" class="form-control" name="b_StaffNo" placeholder="@OABordrinCommon.Common.GetLanguageValueByParam("必填项", "CommonName", "Common", ViewBag.language)" isnotnull="true" disabled="disabled" />
                        </div>
                    </div>
