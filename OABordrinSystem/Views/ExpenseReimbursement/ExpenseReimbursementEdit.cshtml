                    <div class="table-responsive">
                        <table id="ReimbursementItem" class="table table-bordered" style="text-align:left;min-width:1150px;">
                            <thead>
                                <tr>
                                    <th style="text-align: center; background-color: white;width:10%">@OABordrinCommon.Common.GetLanguageValueByParam("日期", "b_ReimbursementItem", "ERItemType", ViewBag.language)</th>
