                            <table id="LoanItem" class="table table-bordered" style="text-align:left;min-width:1150px;">
                                <thead>
                                    <tr>
                                        <th style="text-align: center; background-color: white;">@OABordrinCommon.Common.GetLanguageValueByParam("借款单单号", "b_LoanItem", "ERItemType", ViewBag.language)</th>
